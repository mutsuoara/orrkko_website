<%= form_with(model: event) do |f| %>
<div class="field">
    <div>
    <%= f.label :title, style: "display: block" %>
    <%= f.text_field :title%>
    </div>
    <div>
    <%= f.label :content, style: "display: block" %>
    <%= f.rich_text_area :content %>
    </div>

    <br>
    <div>
    <%= f.submit %>
    </div>
</div>
<% end %>

