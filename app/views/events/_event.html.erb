<div class="container">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Date</th>
        <th>Location</th>
        <th>Phone Number</th>
        <th colspan="4"></th>
      </tr>
    </thead>

    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= link_to event.title, event %></td>
          <td><%= local_time event.date, format: '%B %e, %Y %l:%M %P' %></td>
          <td><%= event.location %></td>
          <td><%= event.phone_number %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>

